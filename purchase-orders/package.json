{
  "name": "purchase-orders",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@sap/cds": "^9",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2",
    "cds-plugin-ui5": "^0.13.0"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-purchase-orders": "cds watch --open com.uvs.praneeth.purchaseorders/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "private": true,
  "cds": {
    "requires": {
      "db": {
        "[development]": {
          "kind": "sqlite",
          "credentials": {
            "url": "db/bussiness.sqlite"
          }
        }
      }
    }
  },
  "[production]": {
    "cds": {
      "requires": {
        "db": {
          "kind": "hana"
        }
      }
    }
  },
  "workspaces": [
    "app/*"
  ],
  "sapux": [
    "app/purchase-orders"
  ]
}
